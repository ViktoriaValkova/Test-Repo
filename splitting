using System;
using System.Linq;

class Program
{
    static void Main()
    {
        string input = "Hello World";

        int firstSpaceIndex = input.IndexOf(' ');

)
        {
            string firstPart = input.Substring(0, firstSpaceIndex);
            string secondPart = input.Substring(firstSpaceIndex + 1);

            Console.WriteLine("First Part: " + firstPart);
            Console.WriteLine("Second Part: " + secondPart);
        }
        else
        {
            Console.WriteLine("Input string does not contain a space to split.");
        }
    }
}

